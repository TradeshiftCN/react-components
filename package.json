{
  "name": "@tradeshift/react-components",
  "version": "0.0.0-semantically-released",
  "description": "React components library",
  "author": "Tradeshift",
  "license": "MIT",
  "main": "dist/react-components.js",
  "module": "dist/react-components.js",
  "types": "types/index.d.ts",
  "repository": "https://github.com/Tradeshift/react-components",
  "homepage": "https://tradeshift.github.io/react-components",
  "engines": {
    "node": ">=8.12.0",
    "npm": ">=6.4.0"
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "create-component": "node scripts/create-component",
    "start": "npm run styleguide",
    "styleguide": "styleguidist server",
    "styleguide:build": "rimraf ./styleguide-dist && cross-env NODE_ENV=production styleguidist build",
    "gh-pages": "npm run styleguide:build && gh-pages -d styleguide-dist --message 'auto-deployed by gh-pages'",
    "build": "rimraf ./dist ./types && tsc --emitDeclarationOnly && cross-env NODE_ENV=production node scripts/build",
    "lint": "tradeshift-scripts lint",
    "format": "tradeshift-scripts format",
    "precommit": "tradeshift-scripts precommit",
    "test": "jest --config=jest.config.json",
    "report-coverage": "codecov",
    "validate": "tradeshift-scripts validate"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0",
    "prop-types": ">=15.0.0"
  },
  "dependencies": {
    "@babel/runtime": "7.x",
    "classnames": "2.x",
    "lodash": "4.x",
    "rc-drawer": "3.x",
    "rc-table": "6.x",
    "rc-tabs": "9.x"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@tradeshift/tradeshift-ui": "^12.3.2",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.144",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "codecov": "^3.6.5",
    "core-js": "^3.3.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "cz-conventional-changelog": "^3.0.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.2",
    "eslint": "^6.5.1",
    "file-loader": "^4.2.0",
    "flush-promises": "^1.0.2",
    "gh-pages": "^2.1.1",
    "glob": "^7.1.4",
    "jest": "^24.9.0",
    "jest-css-modules": "^2.1.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "mustache": "^3.1.0",
    "postcss": "^7.0.18",
    "postcss-load-config": "^2.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "prompts": "^2.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-styleguidist": "^9.1.16",
    "rimraf": "^3.0.0",
    "style-loader": "^1.0.0",
    "tradeshift-scripts": "^2.2.4",
    "typescript": "^3.6.4",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.1",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}
